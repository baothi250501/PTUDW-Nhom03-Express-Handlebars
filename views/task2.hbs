<!--contain-->
    <div class="container">
        <div class="page-header">
            <form class="form-horizontal" method="get" action="">
                <fieldset>
                    <!-- Form Name -->
                    <legend class="text-center">
                        <h1>Jars Saving</h1>
                    </legend>

                    <!-- Text input-->
                    <div class="form-group">
                        <label class="col-md-4 control-label" for="salary">Your salary</label>
                        <div class="col-md-4">
                            <input id="salary" name="salary" type="number" min="0" placeholder=""
                                class="form-control input-md" required="">
                        </div>
                    </div>

                    <!-- Button -->
                    <div class="form-group">
                        <label class="col-md-4 control-label" for="submit"></label>
                        <div class="col-md-4">
                            <input type="submit" value="Calculate" class="btn btn-primary" />
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
        <div class="container text-center">
            <div class="row stylish-panel">
                {{#each jars}}
                <div class="col-md-4">
                    <img src="images{{imagePath}}" alt="{{title}}" class="img-circle img-thumbnail">
                    <h3>{{name}}<small>{{cntValue}}%</small></h3>
                    <h2 class="jars"></h2>
                </div>
                {{/each}}
        </div>
    </div>
    <!-- /contain -->